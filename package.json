{
  "name": "mock-json-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node ./bin/www",
    "build": "babel src --out-dir dist",
    "start": "node ./bin/www",
    "test": "mocha --require @babel/register ./**/*.spec.js --reporter=landing",
    "test:tdd": "mocha --require @babel/register ./**/*.spec.js --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "faker": "^4.1.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.0",
    "nodemon": "^1.18.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "chai": "^4.2.0",
    "mocha": "^6.1.2",
    "nyc": "^13.3.0"
  }
}
